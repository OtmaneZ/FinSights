# EXEMPLE - Copiez ce fichier en .env et modifiez les valeurs

# Google Sheet ID (depuis l'URL)
SPREADSHEET_ID=votre_sheet_id_ici

# Intervalle de polling (secondes)
POLL_INTERVAL=30

# Noms des onglets
SHEET_NAME_FACTURES=Factures
SHEET_NAME_ENCAISSEMENTS=Encaissements

# Options
AUTO_POLLING_ENABLED=true
DEBUG_MODE=true
